{
  "name": "veact-admin",
  "version": "1.0.0",
  "author": {
    "name": "Surmon",
    "email": "surmon@foxmail.com",
    "url": "https://surmon.me"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/surmon-china/veact-admin.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:proxy": "PROXY=true vite",
    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,mjs,ts,tsx}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/*.{css,scss,less,json,html}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ],
    "*.md|{.github,benchmark,bin,examples,hot,lib,schemas,setup,tooling}/**/*.{md,yml,yaml,js,json}": [
      "cspell"
    ]
  },
  "dependencies": {
    "@vue/reactivity": "^3.1.1",
    "ali-oss": "^6.15.2",
    "antd": "^4.16.2",
    "axios": "^0.21.1",
    "bezier-easing": "^2.1.0",
    "classnames": "^2.3.1",
    "gravatar": "^1.8.1",
    "js-base64": "^3.6.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "performant-array-to-tree": "^1.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.2",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.8",
    "@types/gravatar": "^1.8.1",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2",
    "vite": "^2.3.7"
  }
}
